{
  "name": "ts-demo",
  "version": "1.0.0",
  "description": "Projecto de ejemplo fundamentos de TS",
  "main": "index.js",
  "scripts": {
    "tsNode": "cd src && ts-node index.ts",
    "start": "nodemon",
    "linting": "eslint",
    "linting:fix": "eslint --fix",
    "document": "typedoc --out docs src",
